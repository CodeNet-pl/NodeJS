{
  "name": "@code-net/hal-api",
  "version": "1.3.0",
  "type": "commonjs",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "typesVersions": {
    "*": {
      "express": [
        "./src/express.d.ts"
      ],
      "nestjs": [
        "./src/nestjs.d.ts"
      ],
      "fastify": [
        "./src/fastify.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./src/index.js",
      "types": "./src/index.d.ts"
    },
    "./express": {
      "import": "./src/express.js",
      "require": "./src/express.js",
      "types": "./src/express.d.ts"
    },
    "./nestjs": {
      "import": "./src/nestjs.js",
      "require": "./src/nestjs.js",
      "types": "./src/nestjs.d.ts"
    },
    "./fastify": {
      "import": "./src/fastify.js",
      "require": "./src/fastify.js",
      "types": "./src/fastify.d.ts"
    }
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "express": ">=4.0.0",
    "@nestjs/common": ">=9.0.0",
    "fastify": ">=4.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@nestjs/testing": "9.4.3",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.1",
    "fastify": ">=4.0.0",
    "fastify-plugin": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "fastify-plugin": {
      "optional": true
    },
    "@nestjs/common": {
      "optional": true
    }
  }
}
